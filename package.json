{
  "name": "PrescriptText",
  "private": true,
  "scripts": {
    "startdev": "concurrently -k \"pnpm:dev\" \"pnpm:watch\"",
    "watch": "cargo watch -C ./src-wasm -s \"pnpm build\"",
    "dev": "vite",
    "build": "pnpm --filter wasm run build && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "vite": "^7.3.1",
    "vite-plugin-solid": "^2.11.10",
    "vite-plugin-wasm": "^3.5.0"
  },
  "dependencies": {
    "solid-js": "^1.9.11"
  }
}
